#include <iostream>
using namespace std;
int main() 
{
    int n,SP,SI;
    SP=0;
    SI=0;
    cin>>n;
    int Vector[n];
 for(int i=0;i<n;i++){
     Vector[i]=i+1;
 
     if(Vector[i]%2==0){
 SP=SP+Vector[i];
 }
     else{
     SI=SI+Vector[i];
 }
}
   cout<<SP<<endl;
   cout<<SI<<endl;
    return 0;
 }
